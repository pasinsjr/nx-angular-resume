{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "module": "es2015",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@nx-angular-resume/resume/dashboard": [
        "libs/resume/dashboard/src/index.ts"
      ],
      "@nx-angular-resume/resume-ui": ["libs/resume-ui/src/index.ts"],
      "@nx-angular-resume/shared-ui": ["libs/shared-ui/src/index.ts"],
      "@nx-angular-resume/common-classes": ["libs/common-classes/src/index.ts"],
      "@nx-angular-resume/live-chat": ["libs/live-chat/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
